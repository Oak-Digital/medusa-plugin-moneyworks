{
  "name": "@oak-digital/medusa-plugin-moneyworks",
  "version": "0.3.0",
  "description": "",
  "packageManager": "pnpm@8.7.5",
  "author": "Oak Digital",
  "license": "MIT",
  "keywords": [
    "medusa-plugin",
    "medusa-plugin-other",
    "moneyworks"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env pnpm run build:server",
    "_build": "cross-env pnpm run clean && pnpm run build:server && pnpm run build:admin",
    "build:server": "cross-env pnpm run clean && tsc -p tsconfig.json",
    "_build:admin": "cross-env medusa-admin build",
    "release": "bumpp",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env pnpm run build && medusa start",
    "start:custom": "cross-env pnpm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env pnpm run build:server && medusa develop"
  },
  "_olddeps": {
    "@medusajs/cache-redis": "^1.8.8",
    "@medusajs/event-bus-local": "^1.9.6",
    "@medusajs/event-bus-redis": "^1.8.9",
    "@medusajs/file-local": "^1.0.2",
    "@medusajs/icons": "^1.0.0",
    "@medusajs/ui": "^1.0.0",
    "@tanstack/react-query": "4.22.0",
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.17.2",
    "medusa-interfaces": "^1.3.7",
    "prism-react-renderer": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa": "^1.16.0",
    "@types/express": "^4.17.13",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.13",
    "bumpp": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "mongoose": "^5.13.14",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/admin": "^7.1.1",
    "@medusajs/medusa": "^1.16.0",
    "typeorm": "^0.3.16"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@oak-digital/moneyworks": "^0.2.0",
    "html-entities": "^2.4.0",
    "http-status-codes": "^2.3.0",
    "zod": "~3.22.2",
    "zod-express-middleware": "^1.4.0"
  }
}
